        }
      },
      {
        "command": "im-too-lazy.goTagCurrentAll",
        "title": "Convert to All Tags (Current Struct)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.goTagAllJsonBson",
        "title": "Convert to json+bson Tags (All Structs)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
