{
  "name": "im-too-lazy",
  "publisher": "selcuksarikoz",
  "displayName": "I'm Too Lazy",
  "description": "VS Code JSON converter for Go, TypeScript, Rust, and Python with selection-aware fragments, TypeScript Zod/Prisma/Drizzle generation, Go struct tag automation, Python property getter/setter generation, and workspace setup tools (.editorconfig, .prettierrc, uv init + venv).",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Selcuk Sarikoz",
  "repository": {
    "type": "git",
    "url": "https://github.com/selcuksarikoz/im-too-lazy.git"
  },
  "homepage": "https://github.com/selcuksarikoz/im-too-lazy#readme",
  "funding": "https://buymeacoffee.com/funnyturkishdude",
  "bugs": {
    "url": "https://github.com/selcuksarikoz/im-too-lazy/issues"
  },
  "keywords": [
    "vscode",
    "extension",
    "json to go",
    "json to typescript",
    "json to prisma",
    "json to drizzle",
    "go struct tags",
    "gorm",
    "xorm",
    "bson",
    "prettier",
    "editorconfig",
    "python",
    "pydantic",
    "dataclass",
    "typeddict",
    "uv",
    "venv",
    "zod",
    "validate",
    "required",
    "rust",
    "prisma",
    "drizzle",
    "orm"
  ],
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:rust",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:go",
    "onLanguage:python",
    "onCommand:im-too-lazy.jsonConvertMenu",
    "onCommand:im-too-lazy.jsonToGo",
    "onCommand:im-too-lazy.jsonToTypeScript",
    "onCommand:im-too-lazy.jsonToTypeScriptZod",
    "onCommand:im-too-lazy.jsonToRust",
    "onCommand:im-too-lazy.jsonToPythonPydantic",
    "onCommand:im-too-lazy.jsonToPythonDataclass",
    "onCommand:im-too-lazy.jsonToPythonTypedDict",
    "onCommand:im-too-lazy.goTagMenuCurrent",
    "onCommand:im-too-lazy.goTagMenuAll",
    "onCommand:im-too-lazy.goTagCurrentJsonBson",
    "onCommand:im-too-lazy.goTagCurrentAll",
    "onCommand:im-too-lazy.goTagAllJsonBson",
    "onCommand:im-too-lazy.goTagAllAll",
    "onCommand:im-too-lazy.pythonPropsMenuCurrent",
    "onCommand:im-too-lazy.pythonPropsMenuClass",
    "onCommand:im-too-lazy.pythonPropsMenuAll",
    "onCommand:im-too-lazy.createEditorConfig",
    "onCommand:im-too-lazy.createPrettierConfig",
    "onCommand:im-too-lazy.createFormattingFiles",
    "onCommand:im-too-lazy.uvInitPythonProject",
    "onCommand:im-too-lazy.configurePythonVenvSettings",
    "onCommand:im-too-lazy.setupPythonWithUv",
    "onCommand:im-too-lazy.quickActionsMenu",
    "onCommand:im-too-lazy.pasteToMenu"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "im-too-lazy.quickActionsMenu",
        "title": "Quick Actions",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.pasteToMenu",
        "title": "Paste To...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonConvertMenu",
        "title": "Convert...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToGo",
        "title": "Convert to Go",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToTypeScript",
        "title": "Convert to TypeScript",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToTypeScriptZod",
        "title": "Convert to TypeScript (Zod)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToRust",
        "title": "Convert to Rust",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToPythonPydantic",
        "title": "Convert to Python (Pydantic)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToPythonDataclass",
        "title": "Convert to Python (dataclass)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.jsonToPythonTypedDict",
        "title": "Convert to Python (TypedDict)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.goTagMenuCurrent",
        "title": "Convert to Tags (Current Struct)...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.goTagMenuAll",
        "title": "Convert to Tags (All Structs)...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.goTagCurrentJsonBson",
        "title": "Convert to json+bson Tags (Current Struct)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
