        }
      },
      {
        "command": "im-too-lazy.goTagAllAll",
        "title": "Convert to All Tags (All Structs)",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.pythonPropsMenuCurrent",
        "title": "Add Property (Current Property)...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.pythonPropsMenuClass",
        "title": "Add Properties (Current Class)...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.pythonPropsMenuAll",
        "title": "Add Properties (All Classes)...",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.createEditorConfig",
        "title": "Create .editorconfig",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.createPrettierConfig",
        "title": "Create .prettierrc",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.createFormattingFiles",
        "title": "Create .editorconfig + .prettierrc",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.uvInitPythonProject",
        "title": "UV Init Python Project",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.configurePythonVenvSettings",
        "title": "Configure VS Code Python Venv",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      },
      {
        "command": "im-too-lazy.setupPythonWithUv",
        "title": "UV Init + Configure Python Venv",
        "category": "I'm Too Lazy",
        "icon": {
          "light": "icons/icon.svg",
          "dark": "icons/icon.svg"
        }
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "im-too-lazy.jsonConvertMenu",
          "when": "(editorLangId == json || editorLangId == jsonc) && config.imTooLazy.languages.json",
          "group": "navigation"
        },
        {
          "command": "im-too-lazy.goTagMenuCurrent",
          "when": "editorLangId == go && config.imTooLazy.languages.go",
          "group": "navigation"
        },
        {
          "command": "im-too-lazy.pythonPropsMenuCurrent",
          "when": "editorLangId == python && config.imTooLazy.languages.python",
          "group": "navigation"
        },
        {
          "command": "im-too-lazy.pythonPropsMenuClass",
          "when": "editorLangId == python && config.imTooLazy.languages.python",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "submenu": "im-too-lazy.explorerMenu",
          "group": "navigation@40"
        }
      ],
      "im-too-lazy.explorerMenu": [
        {
          "command": "im-too-lazy.createFormattingFiles",
          "group": "1_setup@1"
        },
        {
          "command": "im-too-lazy.createEditorConfig",
          "group": "1_setup@2"
        },
        {
          "command": "im-too-lazy.createPrettierConfig",
          "group": "1_setup@3"
        },
        {
          "command": "im-too-lazy.setupPythonWithUv",
          "group": "2_python@1"
        },
        {
          "command": "im-too-lazy.uvInitPythonProject",
          "group": "2_python@2"
        },
        {
          "command": "im-too-lazy.configurePythonVenvSettings",
          "group": "2_python@3"
        }
      ],
      "commandPalette": [
        {
          "command": "im-too-lazy.quickActionsMenu",
          "when": "config.imTooLazy.languages.json || config.imTooLazy.languages.go || config.imTooLazy.languages.python || config.imTooLazy.languages.typescript || config.imTooLazy.languages.rust"
        },
        {
          "command": "im-too-lazy.jsonConvertMenu",
          "when": "config.imTooLazy.languages.json"
        },
        {
          "command": "im-too-lazy.pasteToMenu",
          "when": "config.imTooLazy.languages.json && (config.imTooLazy.languages.go || config.imTooLazy.languages.typescript || config.imTooLazy.languages.rust || config.imTooLazy.languages.python)"
        },
        {
          "command": "im-too-lazy.goTagMenuCurrent",
          "when": "config.imTooLazy.languages.go"
        },
        {
          "command": "im-too-lazy.goTagMenuAll",
          "when": "config.imTooLazy.languages.go"
        },
        {
          "command": "im-too-lazy.pythonPropsMenuCurrent",
          "when": "config.imTooLazy.languages.python"
        },
        {
          "command": "im-too-lazy.pythonPropsMenuClass",
          "when": "config.imTooLazy.languages.python"
        },
        {
          "command": "im-too-lazy.pythonPropsMenuAll",
          "when": "config.imTooLazy.languages.python"
        },
        {
          "command": "im-too-lazy.createEditorConfig"
        },
        {
          "command": "im-too-lazy.createPrettierConfig"
        },
        {
          "command": "im-too-lazy.createFormattingFiles"
        },
        {
          "command": "im-too-lazy.uvInitPythonProject"
        },
        {
          "command": "im-too-lazy.configurePythonVenvSettings"
        },
        {
          "command": "im-too-lazy.setupPythonWithUv"
        }
      ]
    },
    "submenus": [
      {
        "id": "im-too-lazy.explorerMenu",
        "label": "I'm Too Lazy"
      }
    ],
    "configuration": {
      "title": "I'm Too Lazy",
      "properties": {
        "imTooLazy.languages.json": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable JSON/JSONC conversion features."
        },
        "imTooLazy.languages.go": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Go struct tag generation features."
        },
        "imTooLazy.languages.python": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Python property/accessor generation features."
        },
        "imTooLazy.languages.typescript": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable TypeScript/Zod conversion targets and TypeScript quick actions."
        },
        "imTooLazy.languages.rust": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Rust conversion targets and Rust quick actions."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && vite build",
    "watch": "vite build --watch",
    "package": "npm run check-types && vite build",
    "pretest": "npm run compile && npm run test:smoke",
    "test:smoke": "vitest run src/test/smoke.test.ts",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "vsix": "vsce package",
    "release": "npm run compile && npm run vsix -- --no-dependencies"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "icon": "icons/icon.png"
}
